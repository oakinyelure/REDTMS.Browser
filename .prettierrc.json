{
  "singleQuote": true,
  "trailingComma": "es5",
  "useTabs": false,
  "semi": true,
  "bracketSpacing": true,
  "bracketSameLine": true,
  "arrowParens": "always",
  "tabWidth": 4,
  "printWidth": 120,
  "overrides": [
    {
      "files": ["**/*.html"],
      "options": {
        "printWidth": 200
      }
    }
  ]
}
